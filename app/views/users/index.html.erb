<h1>Users Listing Page</h1>
<div align="center">
  <%= will_paginate %>
</div>
    <div align="left">
      <% @users.each do |user| %>
        <ul class="listing">
          <div class="row">
            <div class="well col-md-4 col-md-offset-4">
              <li><%= link_to gravatar_for(user),user_path(user) %></li>
              <li class="article-title">
                <%= link_to user.username,user_path(user) %>
              </li>
              <li><%= pluralize(user.articles.count,"article") if user.articles %> </li></small>
            </div>
          </div>
        </ul>
      <% end %>
    </div>
<div align="center">
  <%= will_paginate %>
</div>
